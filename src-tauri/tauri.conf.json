{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Task Track",
  "mainBinaryName": "Task Track",
  "version": "0.1.1",
  "identifier": "TaskTrack",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Task Track",
        "width": 800,
        "height": 600,
        "resizable": true,
        "minWidth": 340,
        "minHeight": 300
      }
    ]
  }
}